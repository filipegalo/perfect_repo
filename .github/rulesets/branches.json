{
    "name": "Branches",
    "target": "branch",
    "source_type": "Repository",
    "enforcement": "active",
    "conditions": {
        "ref_name": {
            "exclude": [],
            "include": [
                "refs/heads/release/*",
                "refs/heads/feature/*",
                "refs/heads/hotfix/*"
            ]
        }
    }
}
