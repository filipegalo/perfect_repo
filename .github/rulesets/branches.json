{
    "name": "Branches",
    "target": "branch",
    "source_type": "Repository",
    "enforcement": "active",
    "conditions": {
        "ref_name": {
            "exclude": [],
            "include": [
                "refs/heads/release/*",
                "refs/heads/feature/*",
                "refs/heads/hotfix/*"
            ]
        }
    },
    "rules": [
        {
            "type": "required_status_checks",
            "parameters": {
                "required_status_checks": [
                    {
                        "context": "*"
                    }
                ],
                "strict_required_status_checks_policy": true
            }
        }
    ]
}
